{
	"name": "new-years-functions",
	"version": "0.0.0",
	"license": "MIT",
	"dockerHubRepository": "haydenbr/new-years-functions",
	"scripts": {
		"start": "docker-compose -f ./docker-compose.dev.yml up",
		"down": "docker-compose -f ./docker-compose.dev.yml down",
		"bump": "./scripts/bump.js",
		"docker-build": "docker build -t $(./scripts/current-tag.js) .",
		"docker-push": "docker push $(./scripts/current-tag.js)",
		"major": "yarn bump major",
		"minor": "yarn bump minor",
		"patch": "yarn bump patch",
		"precommit": "pretty-quick --staged"
	},
	"devDependencies": {
		"chalk": "2.3.0",
		"husky": "0.14.3",
		"js-yaml": "3.10.0",
		"lodash": "4.17.5",
		"prettier": "1.10.2",
		"pretty-quick": "1.2.3"
	}
}
